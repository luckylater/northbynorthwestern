<div class="article-page">
  <div class="article-page">
    <div class="nav-sidebar">
      {{#get "tags" limit="all"}}
        {{#foreach tags}}
          {{> "nav-sidebar-sections" }}
        {{/foreach}}
      {{/get}}
    </div>
  </div>
  {{#post}}
    <article>
      <div class="article-container">
        {{> "contact-body"}}
      </div>
    </article>
  {{#get "authors" limit="all"}}
    {{authors separator="<br>"}}
  {{/get}}
  {{/post}}
</div>
